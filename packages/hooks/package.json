  "files": [
    "./dist"
  ],
  "exports": {
    "./use-keyboard-navigation": {
      "import": "./dist/useKeyboardNavigation.js",
      "require": "./dist/useKeyboardNavigation.cjs",
      "types": "./dist/useKeyboardNavigation.d.ts"
    },
    "./use-click-outside": {
      "import": "./dist/useClickOutside.js",
      "require": "./dist/useClickOutside.cjs",
      "types": "./dist/useClickOutside.d.ts"
    },
    "./use-debounce": {
      "import": "./dist/useDebounce.js",
      "require": "./dist/useDebounce.cjs",
      "types": "./dist/useDebounce.d.ts"
    },
    "./use-controlled-state": {
      "import": "./dist/useControlledState.js",
      "require": "./dist/useControlledState.cjs",
      "types": "./dist/useControlledState.d.ts"
    },
    "./use-toggle-visibility": {
      "import": "./dist/useToggleVisibility.js",
      "require": "./dist/useToggleVisibility.cjs",
      "types": "./dist/useToggleVisibility.d.ts"
    },
    "./use-multi-select": {
      "import": "./dist/useMultiSelect.js",
      "require": "./dist/useMultiSelect.cjs",
      "types": "./dist/useMultiSelect.d.ts"
    },
    "./use-pagination": {
      "import": "./dist/usePagination.js",
      "require": "./dist/usePagination.cjs",
      "types": "./dist/usePagination.d.ts"
    },
    "./use-counter": {
      "import": "./dist/useCounter.js",
      "require": "./dist/useCounter.cjs",
      "types": "./dist/useCounter.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "exports": {
      "./use-keyboard-navigation": {
        "import": "./dist/useKeyboardNavigation.js",
        "require": "./dist/useKeyboardNavigation.cjs",
        "types": "./dist/useKeyboardNavigation.d.ts"
      },
      "./use-click-outside": {
        "import": "./dist/useClickOutside.js",
        "require": "./dist/useClickOutside.cjs",
        "types": "./dist/useClickOutside.d.ts"
      },
      "./use-debounce": {
        "import": "./dist/useDebounce.js",
        "require": "./dist/useDebounce.cjs",
        "types": "./dist/useDebounce.d.ts"
      },
      "./use-controlled-state": {
        "import": "./dist/useControlledState.js",
        "require": "./dist/useControlledState.cjs",
        "types": "./dist/useControlledState.d.ts"
      },
      "./use-toggle-visibility": {
        "import": "./dist/useToggleVisibility.js",
        "require": "./dist/useToggleVisibility.cjs",
        "types": "./dist/useToggleVisibility.d.ts"
      },
      "./use-multi-select": {
        "import": "./dist/useMultiSelect.js",
        "require": "./dist/useMultiSelect.cjs",
        "types": "./dist/useMultiSelect.d.ts"
      },
      "./use-pagination": {
        "import": "./dist/usePagination.js",
        "require": "./dist/usePagination.cjs",
        "types": "./dist/usePagination.d.ts"
      },
      "./use-counter": {
        "import": "./dist/useCounter.js",
        "require": "./dist/useCounter.cjs",
        "types": "./dist/useCounter.d.ts"
      },
      ".": {
        "import": "./dist/*.js",
        "require": "./dist/*.cjs",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage}"
  },
  "peerDependencies": {
    "react": "^19.2.0",
