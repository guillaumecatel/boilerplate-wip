    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.13",
    "jsdom": "^27.2.0",
    "tsdown": "^0.16.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
