{
  "name": "@myorg/shared",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "Shared utilities and types for the monorepo.",
  "keywords": [
    "shared",
    "utilities",
    "typescript"
  ],
  "homepage": "https://github.com/myorg/monorepo/packages/shared#readme",
  "author": "My Org <contact@myorg.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/myorg/monorepo.git"
  },
  "bugs": {
    "url": "https://github.com/myorg/monorepo/issues"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    "./alias": "./src/alias.ts",
    "./dom": "./src/dom.ts",
    "./guards": "./src/guards.ts",
    "./intl": "./src/intl.ts",
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "exports": {
      "./alias": {
        "import": "./dist/alias.js",
        "require": "./dist/alias.cjs",
        "types": "./dist/alias.d.ts"
      },
      "./dom": {
        "import": "./dist/dom.js",
        "require": "./dist/dom.cjs",
        "types": "./dist/dom.d.ts"
      },
      "./guards": {
        "import": "./dist/guards.js",
        "require": "./dist/guards.cjs",
        "types": "./dist/guards.d.ts"
      },
      "./intl": {
        "import": "./dist/intl.js",
        "require": "./dist/intl.cjs",
        "types": "./dist/intl.d.ts"
      },
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage}"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.13",
    "jsdom": "^27.2.0",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
