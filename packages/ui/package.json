    "./divider": "./src/Divider.tsx",
    "./definition-list": "./src/DefinitionList.tsx",
    "./checkbox": "./src/Checkbox.tsx",
    "./button-group": "./src/ButtonGroup.tsx",
    "./alert": "./src/Alert.tsx",
    "./bread-crumb": "./src/BreadCrumb.tsx",
    "./badge": "./src/Badge.tsx",
    "./button": "./src/Button.tsx",
    ".": "./src/index.ts"
