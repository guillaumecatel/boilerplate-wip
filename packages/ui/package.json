    "url": "https://github.com/myorg/monorepo/issues"
  },
  "files": [
    "./dist"
  ],
  "exports": {
    "./text": "./src/Text.tsx",
    "./accordion": "./src/Accordion.tsx",
    "./toggle": "./src/Toggle.tsx",
    "./textarea": "./src/Textarea.tsx",
    "./table": "./src/Table.tsx",
