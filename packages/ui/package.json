  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage}"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@myorg/shared": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "radix-ui": "^1.4.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "^27.0.0",
