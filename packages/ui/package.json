{
  "name": "@myorg/ui",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "React components library for building user interfaces.",
  "keywords": [
    "ui",
    "components",
    "library",
    "react"
  ],
  "homepage": "https://github.com/myorg/monorepo/packages/ui#readme",
