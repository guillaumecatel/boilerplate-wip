  "author": "My Org <contact@myorg.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/myorg/my-repo.git"
  },
  "bugs": {
    "url": "https://github.com/myorg/my-repo/issues"
  },
  "files": ["./dist"],
  "exports": {
    "./button": "./src/Button.tsx",
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "exports": {
      "./button": {
        "import": "./dist/Button.js",
        "require": "./dist/Button.cjs",
        "types": "./dist/Button.d.ts"
      },
      ".": {
        "import": "./dist/*.js",
        "require": "./dist/*.cjs",
        "types": "./dist/*.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "shx rm -rf {node_modules,.turbo,dist,coverage}"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "^27.0.0",
    "tsdown": "^0.15.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
